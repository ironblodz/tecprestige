			<fields-in-item items_of="tabs">
				<field>tab_title</field>
				<field editor_type="AREA">tab_content</field>
			</fields-in-item>
		</widget>
		<widget name="konte-banner-carousel">
			<fields-in-item items_of="banners">
				<field>title</field>
				<field editor_type="AREA">description</field>
				<field>button_text</field>
				<field editor_type="LINK">link>url</field>
			</fields-in-item>
		</widget>
		<widget name="konte-banner-grid">
			<fields>
				<field>banner_1_tagline</field>
				<field>banner_1_title</field>
				<field>banner_1_button_text</field>
				<field editor_type="LINK">banner_1_link>url</field>
				<field>banner_2_tagline</field>
