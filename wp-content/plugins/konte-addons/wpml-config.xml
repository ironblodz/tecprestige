<wpml-config>
	<custom-types>
		<custom-type translate="1">konte_size_guide</custom-type>
		<custom-type translate="1">flex_post</custom-type>
		<custom-type translate="1">portfolio</custom-type>
	</custom-types>
	<taxonomies>
		<taxonomy translate="1">flex_post_tag</taxonomy>
		<taxonomy translate="1">portfolio_type</taxonomy>
		<taxonomy translate="1">portfolio_tag</taxonomy>
	</taxonomies>
	<custom-fields>
		<custom-field action="copy-once">size_guides</custom-field>
		<custom-field action="copy-once">size_guide_category</custom-field>
		<custom-field action="copy-once">konte_size_guide</custom-field>
		<custom-field action="copy">flex_content_type</custom-field>
		<custom-field action="translate">flex_post_custom</custom-field>
		<custom-field action="translate">flex_post_instagram</custom-field>
		<custom-field action="copy">flex_post_post</custom-field>
		<custom-field action="copy-once">flex_post_design</custom-field>
	</custom-fields>
	<shortcodes>
